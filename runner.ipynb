   "source": [
    "nqbits = 5\n",
    "\n",
    "q = qprogram(nqbits)\n",
    "\n",
    "q.addgates(0, [HGATE()])\n",
    "\n",
    "for each in range(1, nqbits):\n",
    "    for i in range(each):\n",
    "        q.addgates(each, [IGATE()])\n",
    "    q.addgates(each, [CNOTGATE()])\n",
    "\n",
    "q.compile()"
   ]
